<html>

<head>
    <title>Spring Boot Mustache</title>
    <style>
        table {
            border-collapse: collapse;
        }

        table, td, th {
            border: 1px solid #999;
            padding: 5px;
        }
    </style>
</head>

<body>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>LastName</th>
        <th>Age</th>
        <th>PurchaseItem</th>
        <th>Count</th>
        <th>Amount</th>
        <th>PurchaseDate</th>
    </tr>

    {{#sales}}
    <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{lastName}}</td>
            <td>{{age}}</td>
            <td>{{purchase_item}}</td>
            <td>{{count}}</td>
            <td>{{amount}}</td>
            <td>{{purchase_date}}</td>
    <tr>
    {{/sales}}

</table>
</body>
</html>